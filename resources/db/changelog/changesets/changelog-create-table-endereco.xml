<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="GABRIEL">
        <createTable tableName="endereco">
            <column name="endereco_id" type="INTEGER" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_ENDERECO"  />
            </column>
            <column name="rua" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="numero" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="bairro" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
            <column name="cidade" type="VARCHAR(255)">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>